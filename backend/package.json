{
  "name": "doup-staff",
  "version": "0.0.1",
  "description": "Backend service to manage DOUP! staff requirements.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "test": "xo && ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tmilar/doup-staff.git"
  },
  "keywords": [
    "node.js",
    "google-drive"
  ],
  "author": "tmilar",
  "bugs": {
    "url": "https://github.com/tmilar/doup-staff/issues"
  },
  "homepage": "https://github.com/tmilar/doup-staff#readme",
  "devDependencies": {
    "ava": "1.3.1",
    "xo": "0.24.0"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "rules": {
      "camelcase": "warn",
      "no-use-extend-native/no-use-extend-native": "warn",
      "import/no-unassigned-import": "off",
      "new-cap": 0
    }
  },
  "dependencies": {
    "bcrypt": "2.0.1",
    "bluebird": "3.5.3",
    "body-parser": "1.18.3",
    "cors": "2.8.5",
    "debug": "4.1.1",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "googleapis": "37.2.0",
    "mongoose": "5.4.18",
    "multer": "1.4.1"
  }
}
