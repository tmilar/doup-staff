    "test": "xo && ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tmilar/doup-staff.git"
  },
  "keywords": [
    "node.js",
    "google-drive"
  ],
  "author": "tmilar",
  "bugs": {
    "url": "https://github.com/tmilar/doup-staff/issues"
  },
  "homepage": "https://github.com/tmilar/doup-staff#readme",
  "devDependencies": {
    "ava": "1.3.1",
    "xo": "0.24.0"
