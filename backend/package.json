  "xo": {
    "space": true,
    "semicolon": false,
    "rules": {
      "camelcase": "warn",
      "no-use-extend-native/no-use-extend-native": "warn",
      "import/no-unassigned-import": "off",
      "new-cap": 0
    }
  },
  "dependencies": {
    "dotenv": "6.2.0",
    "googleapis": "37.2.0",
    "opn": "5.4.0",
    "server-destroy": "1.0.1"
